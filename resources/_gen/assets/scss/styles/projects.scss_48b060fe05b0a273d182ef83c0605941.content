@import "https://fonts.googleapis.com/css2?family=Ubuntu&display=swap";:root{--theme:rgb(245, 245, 245);--primary:rgb(30, 30, 30);--secondary:rgba(108, 108, 108);--entry:rgb(255, 255, 255);--border_color:rgba(229,229,229,1);--border_color_hover:rgba(212,212,212,1);--inputs_bg:rgba(245,245,245,1);--border_line_color:rgba(229,229,229,1);--sun_icon_visibility:none;--moon_icon_visibility:block;--filter_invert:invert(0)}.darkmode{--theme:rgb(29, 30, 32);--primary:rgb(218, 218, 219);--secondary:rgba(155, 156, 157);--entry:rgb(46, 46, 51);--border_color:rgba(38,38,38,1);--border_color_hover:rgba(64,64,64,1);--inputs_bg:rgba(38,38,38,1);--border_line_color:rgba(38,38,38,1);--sun_icon_visibility:block;--moon_icon_visibility:none;--filter_invert:invert(1)}.js,.javascript,.Javascript{background-color:#f0db4f!important;color:#12181a!important}.react,.React{background-color:#61dbfb!important;color:#12181a!important}.python,.py,.Python{background-color:#306998!important;color:#fff!important}.html,.HTML{background-color:#e34c26!important;color:#12181a!important}.css,.CSS{background-color:#2965f1!important;color:#fff!important}.scss,.SCSS,.sass,.SASS{background-color:#c69!important;color:#12181a!important}.node,.nodejs,.Nodejs,.NodeJs{background-color:#3c873a!important;color:#fff!important}section#projects{padding:2rem 0 3rem;list-style:none}section#projects ul.projects{display:grid;grid-template-columns:repeat(3,minmax(0,1fr));gap:1rem}@media all and (max-width:768px){section#projects ul.projects{grid-template-columns:repeat(2,minmax(0,1fr))}}@media all and (max-width:640px){section#projects ul.projects{grid-template-columns:repeat(1,minmax(0,1fr))}}section#projects ul.projects li.project{border:1px solid;border-color:var(--border_color);border-radius:.3rem;transition-property:all;transition-timing-function:cubic-bezier(.4,0,.2,1);transition-duration:.15s;padding:.5rem;display:flex;flex-direction:column;justify-content:space-between}section#projects ul.projects li.project:hover{border-color:var(--border_color_hover)}section#projects ul.projects li.project h1.title{display:flex;align-items:center}section#projects ul.projects li.project h1.title a{font-size:1.1rem;font-weight:600;color:#2dd4bf}section#projects ul.projects li.project h1.title span{display:block;margin-right:.8rem}section#projects ul.projects li.project h1.title span img{display:block;filter:var(--filter_invert)}section#projects ul.projects li.project p.description{font-size:1rem;font-weight:400}section#projects ul.projects li.project nav{width:100%;display:flex;justify-content:flex-end;align-items:center}section#projects ul.projects li.project nav a{display:block;font-size:.9rem;margin-right:0;font-weight:600;padding:0 .7rem;border:3px solid transparent;color:#2dd4bf}section#projects ul.projects li.project nav a:last-child{margin-right:0}section#projects ul.projects li.project ul.tags{display:flex;align-items:center}section#projects ul.projects li.project ul.tags li.tag{margin-right:.9rem;display:flex;align-items:center}section#projects ul.projects li.project ul.tags li.tag:last-child{margin-right:0}section#projects ul.projects li.project ul.tags li.tag span.label{display:block;width:.7rem;height:.7rem;border-radius:50%;margin-right:.6rem}section#projects ul.projects li.project ul.tags li.tag a{display:block;font-size:.9rem;font-weight:600;color:var(--primary)}section#projects div.view_all{text-align:center;padding-top:2rem}section#projects div.view_all a.button{display:inline-block;margin:0 auto;background:#2dd4bf;color:#171717;padding:.1rem .6rem;border-radius:.5rem;transition-property:all;transition-timing-function:cubic-bezier(.4,0,.2,1);transition-duration:.15s;font-size:1rem;border:3px solid transparent;font-weight:700}section#projects div.view_all a.button:focus-within{border-color:#364fc7}