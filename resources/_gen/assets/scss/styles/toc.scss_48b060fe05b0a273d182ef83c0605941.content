@import "https://fonts.googleapis.com/css2?family=Ubuntu&display=swap";:root{--theme:rgb(245, 245, 245);--primary:rgb(30, 30, 30);--secondary:rgba(108, 108, 108);--entry:rgb(255, 255, 255);--border_color:rgba(229,229,229,1);--border_color_hover:rgba(212,212,212,1);--inputs_bg:rgba(245,245,245,1);--border_line_color:rgba(229,229,229,1);--sun_icon_visibility:none;--moon_icon_visibility:block;--filter_invert:invert(0)}.darkmode{--theme:rgb(29, 30, 32);--primary:rgb(218, 218, 219);--secondary:rgba(155, 156, 157);--entry:rgb(46, 46, 51);--border_color:rgba(38,38,38,1);--border_color_hover:rgba(64,64,64,1);--inputs_bg:rgba(38,38,38,1);--border_line_color:rgba(38,38,38,1);--sun_icon_visibility:block;--moon_icon_visibility:none;--filter_invert:invert(1)}#toc_container{margin:2rem 0 0;background:var(--theme)repeat;display:table;border:none;font-size:95%;border-radius:.3rem;width:auto;border-radius:3px;padding:.9rem 15px;box-shadow:#0a0909d2 0 10px 20px}#toc_container .title{font-size:1.1rem;font-weight:700}#toc_container a{color:#059669;text-decoration:underline}#toc_container nav#TableOfContents{padding:.3rem 15px}#toc_container nav#TableOfContents ul li ul li{margin-left:1rem}