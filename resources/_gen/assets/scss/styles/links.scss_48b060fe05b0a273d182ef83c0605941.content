@import "https://fonts.googleapis.com/css2?family=Ubuntu&display=swap";:root{--theme:rgb(245, 245, 245);--primary:rgb(30, 30, 30);--secondary:rgba(108, 108, 108);--entry:rgb(255, 255, 255);--border_color:rgba(229,229,229,1);--border_color_hover:rgba(212,212,212,1);--inputs_bg:rgba(245,245,245,1);--border_line_color:rgba(229,229,229,1);--sun_icon_visibility:none;--moon_icon_visibility:block;--filter_invert:invert(0)}.darkmode{--theme:rgb(29, 30, 32);--primary:rgb(218, 218, 219);--secondary:rgba(155, 156, 157);--entry:rgb(46, 46, 51);--border_color:rgba(38,38,38,1);--border_color_hover:rgba(64,64,64,1);--inputs_bg:rgba(38,38,38,1);--border_line_color:rgba(38,38,38,1);--sun_icon_visibility:block;--moon_icon_visibility:none;--filter_invert:invert(1)}section#links{padding:2rem 0 3rem;line-height:2rem;text-align:center}section#links h3.head_title{text-decoration:none;font-weight:700;font-size:2rem}section#links ul.links{width:30%;margin:0 auto;list-style:none}@media all and (max-width:980px){section#links ul.links{width:50%}}@media all and (max-width:640px){section#links ul.links{width:80%}}section#links ul.links li.link{width:100%;padding:.4rem .1rem;border-radius:.3rem;background:var(--entry);margin-bottom:20px;text-align:center}section#links ul.links li.link:last-child{margin-bottom:0}section#links ul.links li.link a{display:block;font-size:1rem;color:var(--primary);font-weight:700}